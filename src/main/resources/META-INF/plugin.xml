<idea-plugin>
    <id>org.sdk.popup</id>
    <name>SDK: Action Sample</name>
    <version>1.0.0</version>

    <!-- ONLY use standard dependencies -->
    <depends>com.intellij.modules.platform</depends>

    <!-- Align versions with build.gradle.kts -->
    <idea-version since-build="241.0" until-build="241.*"/>

    <description>
        Demonstrates automatically showing a popup at IDE startup.
    </description>

    <change-notes>
        Initial stable version release.
    </change-notes>

    <vendor email="hamnajabbar36@gmail.com">Hamna Jabbar</vendor>

    <extensions defaultExtensionNs="com.intellij">
        <postStartupActivity implementation="com.intellij.sdk.Main"/>
    </extensions>
</idea-plugin>